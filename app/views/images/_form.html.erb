<%= form_for Image.new do |f| %>
  <%= f.label :image, "Upload pictures:", class: "upload button radius" %>
  <%= f.file_field :image, multiple: true, name: "image[image]", class: "image_upload" %>
<% end %>
<script id="template-upload" type="text/x-tmpl">
<div class="upload">
  {%=o.name%}
  <div class="progress"><span class="meter" style="width: 0%"></span></div>
</div>
</script>